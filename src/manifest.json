{
  "manifest_version": 3,
  "version": "0.3.1",
  "name": "Search On Site",
  "description": "Extension allows do search on a specific site",
  "icons": {
    "16": "icon128.png",
    "48": "icon128.png",
    "128": "icon128.png"
  },
  "background": {
    "service_worker": "index.js",
    "type": "module"
  },
  "action": {
    "default_icon": {
      "19": "icon128.png",
      "38": "icon128.png"
    },
    "default_title": "Click to Search On Site"
  },
  "permissions": ["tabs", "search", "contextMenus"],
  "omnibox": {
    "keyword": "son"
  }
}
