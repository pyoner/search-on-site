{
  "manifest_version": 3,
  "version": "0.4.0",
  "name": "Search On Site",
  "description": "The extension allows you to search on a specific site.",
  "icons": {
    "16": "icon128.png",
    "48": "icon128.png",
    "128": "icon128.png"
  },
  "background": {
    "service_worker": "index.js",
    "type": "module"
  },
  "action": {
    "default_icon": {
      "19": "icon128.png",
      "38": "icon128.png"
    },
    "default_title": "Click to Search On Site"
  },
  "permissions": ["windows", "tabs", "search", "contextMenus"],
  "omnibox": {
    "keyword": "son"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Alt+K"
      }
    }
  }
}
